{
  "version": 2,
  "builds": [
    {
      "src": "client/src/main.server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "server/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "client/src/main.server.ts"
    }
  ]
}